<%= form_with url:'/courses', method: :post, local: true do |form| %>
    <%= form.label :name %>
    <%= form.text_field :name %>
    <%= form.label :par %>
    <%= form.text_field :par %>
    <%= form.label :difficulty %>
    <%= form.text_field :difficulty %>
    <%= form.label :permanent %>
    <%= form.text_field :permanent %>
    <%= form.submit "Create Course" %>
<% end %>
